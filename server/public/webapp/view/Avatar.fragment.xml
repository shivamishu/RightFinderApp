<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Popover
		showHeader="false"
		contentWidth="25rem"
		contentHeight="auto"
		placement="Bottom">
            <List  >
					<StandardListItem
					title="Sign Out"
					type="Active"
					icon="sap-icon://log"
                    infoState="Error"
                    highlight="Error"
					press="onLogoutPress"
					iconDensityAware="true"
					iconInset="true" />
			</List>
        </Popover>
</core:FragmentDefinition>